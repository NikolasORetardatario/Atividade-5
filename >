programa 
{
	funcao inicio() {
		
		real num1
		real num2
		
		escreva("Diga número: ")
		leia(num1)
		
		escreva("Diga outro número: ")
		leia(num2)
		
		se(num1 > num2){
	    escreva(num1, " é maior que ", num2)}
	    
		    senao se (num1 < num2){
		    escreva(num2, " é maior que ", num1)}
		        
		        senao { escreva("Os números são iguais")
		        
	     }
	}
}
